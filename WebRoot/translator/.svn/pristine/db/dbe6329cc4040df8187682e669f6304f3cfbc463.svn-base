@charset "utf-8";
.progress-dzf{
    background-color: #f5f5f5;
   
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1) inset;
    height: 20px;
    margin-bottom: 5px;
    overflow: hidden;
	z-index:-1;
}
.progress-dzf[data-percent]:after {
    bottom: 0;
    color: #fff;
    content: attr(data-percent);
    display: inline-block;
    font-size: 12px;
    left: 0;
    line-height: 16px;
    position: absolute;
    right: 0;
    text-align: center;
    top: 0;
}
.progress-dzf {
    background: none repeat scroll 0 0 #dadada;
    border-radius: 0;
    box-shadow: none;
    height: 18px;
}
